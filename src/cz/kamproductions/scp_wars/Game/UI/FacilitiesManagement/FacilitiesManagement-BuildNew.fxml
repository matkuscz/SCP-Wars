<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="482.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label text="Construct new facility" BorderPane.alignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </top>
   <right>
      <VBox prefHeight="446.0" prefWidth="203.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Free real estate (m2):">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="freeRealEstateLabel" text="0">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="Funds ($$$):">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="fundsLabel" text="0">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="moreInfoButton" mnemonicParsing="false" text="Get more info" />
            <Button fx:id="buyButton" mnemonicParsing="false" text="Buy" textAlignment="CENTER" />
         </children>
      </VBox>
   </right>
   <bottom>
      <Button fx:id="closeButton" mnemonicParsing="false" text="CLOSE" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <left>
      <TableView fx:id="buildFacilityTableView" prefHeight="389.0" prefWidth="463.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </left>
</BorderPane>
