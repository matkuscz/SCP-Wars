<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="buildingManagementBorderPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label text="Building Management" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button fx:id="closeButton" mnemonicParsing="false" text="Close" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <VBox fx:id="centerVBox" minWidth="180.0" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="rentButton" alignment="CENTER" contentDisplay="CENTER" minWidth="175.0" mnemonicParsing="false" text="Rent" textAlignment="CENTER" />
            <Button fx:id="buyButton" alignment="CENTER" contentDisplay="CENTER" minWidth="175.0" mnemonicParsing="false" text="Buy" textAlignment="CENTER" />
            <Button fx:id="marketResearchButton" alignment="CENTER" contentDisplay="CENTER" minWidth="175.0" mnemonicParsing="false" prefWidth="175.0" text="Market Research" />
         </children>
      </VBox>
   </center>
</BorderPane>
